$TTL 3600
$ORIGIN ddns.lab.
@               IN      SOA     ns01.ddns.lab. root.ddns.lab. (
                            2711201408 ; serial
                            3600       ; refresh (1 hour)
                            600        ; retry (10 minutes)
                            86400      ; expire (1 day)
                            600        ; minimum (10 minutes)
                        )

                IN      NS      ns01.ddns.lab.
                IN      NS      ns02.ddns.lab.

; DNS Servers
ns01            IN      A       192.168.56.10
ns02            IN      A       192.168.56.11
